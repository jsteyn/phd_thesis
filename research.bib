@Article{Golowasch1999,
  Title                    = {Activity-dependent regulation of potassium currents in an identified neuron of the stomatogastric ganglion of the crab Cancer borealis.},
  Author                   = {Golowasch, J and Abbott, L F and Marder, E},
  Journal                  = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
