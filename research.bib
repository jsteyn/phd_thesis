@Book{Canepari2011,
  Title                    = {Membrane Potential Imaging in the Nervous System},
  Editor                   = {Canepari, Marco and Zecevic, Dejan},
  Publisher                = {Springer},
  Year                     = {2011},

  Abstract                 = {The fluorescence of dyes added to squid giant axons was studied during action potentials and voltage-clamp steps. One goal was to find fluorescence changes related to the increases in membrane conductance that underlie propagation. A second goal was to find large changes in fluorescence that would allow optical monitoring of membrane potential in neurons and other cells. Attempts were made to measure fluorescence changes using over 300 different fluorescent molecules and positive results were obtained with more than half of these. No evidence was found that would relate, any of the fluorescence changes to the increases in membrane conductance that accompany depolarization; most, instead, were correlated with the changes in membrane potential. The fluorescence changes of several dyes were relatively large; the largest changes during an action potential were 10−3 of the resting intensity. They could be measured with a signal-to-noise ratio of better than 10∶1 in a single sweep.},
  Journal                  = {Second Harmonic Imaging of Membrane Potential,” Springer New York},
  Pages                    = {147--155}
}

@Book{Moss2001,
  Title                    = {Chapter 3 Fluctuations in neural systems: From subcellular to network levels },
  Editor                   = {F. Moss and S. Gielen},
  Publisher                = {North-Holland},
  Year                     = {2001},
  Series                   = {Handbook of Biological Physics },
  Volume                   = {4},

  Abstract                 = {Publisher Summary This chapter reviews some aspects of fluctuations in the nervous systems. Fluctuation analysis is a way to obtain information on low-level processes by studying the activity at a higher level. The chapter focuses on the underlying mechanisms and the possible functional role of fluctuations in the nervous system. The chapter approaches the problem at different complexity levels: (1) the microscopic or molecular level, (2) the mesoscopic or cellular level, and (3) the macroscopic or systems (neural network and brain) level. It discusses the molecular background of stochastic processes in ion-channel activity and gives mathematical descriptions of channel kinetics. It also discusses the functional role of microscopic fluctuations/disorder at the higher, cellular, and systems levels. The chapter investigates the underlying mechanisms of (1) the interval variability by considering channel kinetics and synapse function and (2) amplitude variability by considering some recent studies of hippocampal neurons. Some large-scale simulations of cortical models and the complex dynamics displayed by such systems are reviewed the role of fluctuations for the classical mind-brain problem are discussed. },
  Booktitle                = {Neuro-Informatics and Neural Modelling},
  Doi                      = {http://dx.doi.org/10.1016/S1383-8121(01)80006-3},
  ISSN                     = {1383-8121},
  Owner                    = {jannetta},
  Pages                    = {83 - 129},
  Timestamp                = {2015.04.16},
